#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите первое число: ";
    cin >> a;
    cout << "Введите второе число: ";
    cin >> b;
    cout << "Введите третье число: ";
    cin >> c;
    
    int mid = a;
    
    if (b < a && b > c  b > a && b < c) {
        mid = b;
    }
    if (c < a && c > b  c > a && c < b) {
        mid = c;
    }
    
    cout << "Среднее число: " << mid << endl;
    
    return 0;
}
