#include <iostream>
using namespace std;

int main() {
    int N, seconds;
    cout << "Введите количество секунд, прошедших с начала суток: ";
    cin >> N;

    seconds = N % 60; // остаток от деления на 60 даст количество секунд, прошедших с начала последней минуты

    cout << "С начала последней минуты прошло: " << seconds << " секунд" << endl;

    return 0;
}
