#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите первое число: ";
    cin >> a;
    cout << "Введите второе число: ";
    cin >> b;
    cout << "Введите третье число: ";
    cin >> c;
    
    int min = a;
    
    if (b < min) {
        min = b;
    }
    if (c < min) {
        min = c;
    }
    
    cout << "Наименьшее число: " << min << endl;
    
    return 0;
}
