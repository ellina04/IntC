#include <iostream>
using namespace std;

int main() {
    int N, seconds, minutes;
    cout << "Введите количество секунд, прошедших с начала суток: ";
    cin >> N;

    seconds = N % 3600; // остаток от деления на 3600 даст количество секунд, прошедших с начала последнего часа
    minutes = seconds / 60; // количество полных минут в количестве секунд

    cout << "С начала последнего часа прошло: " << minutes << " минут" << endl;

    return 0;
