#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите три целых числа: ";
    cin >> a >> b >> c;
    if (a == b  b == c  a == c) {
        cout << "Среди трех данных целых чисел есть хотя бы одна пара совпадающих" << endl;
    } else {
        cout << "Среди трех данных целых чисел нет совпадающих пар" << endl;
    }
    return 0;
}
