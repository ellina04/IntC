#include <iostream>
using namespace std;

int main() {
    int num, a, b, c;
    cout << "Введите трехзначное число: ";
    cin >> num;

    a = num / 100;
    b = (num / 10) % 10;
    c = num % 10;

    cout << "Число, полученное при прочтении исходного числа справа налево: " << c << b << a << endl;

    return 0;
}
