#include <iostream>

using namespace std;

int main() {
    int a, b, c;
    int count = 0; // переменная для подсчета количества положительных чисел

    cout << "Введите три целых числа: ";
    cin >> a >> b >> c;

    if (a > 0) {
        count++;
    }
    if (b > 0) {
        count++;
    }
    if (c > 0) {
        count++;
    }

    cout << "Количество положительных чисел: " << count << endl;

    return 0;
}
