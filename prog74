#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Введите три числа: ";
    cin >> a >> b >> c;
    int positiveCount = 0;
    if (a > 0) {
        positiveCount++;
    }
    if (b > 0) {
        positiveCount++;
    }
    if (c > 0) {
        positiveCount++;
    }
    if (positiveCount == 2) {
        cout << "Ровно два из чисел А, В, С положительные" << endl;
    } else {
        cout << "Не ровно два из чисел А, В, С положительные" << endl;
    }
    return 0;
}
